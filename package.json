{
  "name": "alloy-localizer",
  "version": "0.0.1",
  "description": "A Titanium Alloy internationalization plugin that traverses the project directory to create an i18n/en subfolder.",
  "main": "index.js",
  "devDependencies": {
    "chai": "^3.0.0",
    "codeclimate-test-reporter": "^0.1.0",
    "istanbul": "^0.3.17"
  },
  "scripts": {
    "test": "mocha && istanbul report text-summary lcov",
    "main": "index.js",
    "codeclimate-test-reporter": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/codeclimate-test-reporter/bin/codeclimate.js && rm -rf ./coverage",
    "coverall": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anishkhale/alloy-localizer"
  },
  "keywords": [
    "titanium",
    "alloy",
    "internationalization",
    "localization",
    "string",
    "manipulation"
  ],
  "author": "Anish Khale",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anishkhale/alloy-localizer/issues"
  },
  "homepage": "https://github.com/anishkhale/alloy-localizer"
}
